    "scripts": {
        "start": "cross-env TZ=utc ts-node src/server.ts",
        "startBeefy": "cross-env TZ=utc node --max_old_space_size=12192 -r ts-node/register ./src/server.ts",
        "build": "tsc -p .",
        "test": "cross-env TZ=utc jest -i --coverage src/* ",
        "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:add": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
        "rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert",
        "cp": "node --max-old-space-size=8192 dist/cp/index",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
    },
    "_moduleAliases": {
        "@library/*": [
            "./dist/library/*"
        ],
        "@entity/*": [
            "./dist/entity/*"
        ],
        "@rest/*": [
            "./dist/rest/*"
        ],
        "@repository/*": [
            "./dist/repository/*"
        ]
    }
